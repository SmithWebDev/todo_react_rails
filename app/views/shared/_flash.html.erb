<% flash.each do|key, value| %>
  <div class="container">
    <div class="alert <%= key== 'notice' ? 'alert-primary' : 'alert-danger' %> " role="alert">
      <%= value %>
      <button class="close" type="button" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
  </div>
<% end %>
